{
    "name": "tohuwabohu-duplicity",
    "version": "1.0.0",
    "author": "Martin Meinhold",
    "license": "Apache License, Version 2.0",
    "summary": "Install and manage duplicity",
    "source": "https://github.com/tohuwabohu/puppet-duplicity.git",
    "project_page": "https://github.com/tohuwabohu/puppet-duplicity",
